{

  "description": "A modification of open-my-page-button: Adds browser action icon to toolbar to open packaged web page. Adds a purpple, double border, and a pinkc background to all webpages",
  "manifest_version": 2,
  "name": "Open_team_1_homepage",
  "version": "1.0",
  "homepage_url": "https://github.com/hunter-college-ossd-spr-2020/Team_1_add_on, https://github.com/mdn/webextensions-examples/tree/master/open-my-page-button",
  "icons": {
    "48": "icons/image-48.png"
  },
  
   "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["webpage.js"]
    }
  ],

  "background": {
    "scripts": ["background.js"]
  },
  
  "browser_action": {
    "default_icon": "icons/image-32.png"
  }
  
 

}
